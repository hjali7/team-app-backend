{
    "name": "Go Backend (team-app)",
    "image": "mcr.microsoft.com/devcontainers/go:1.22",
    "features": {
        "ghcr.io/devcontainers/features/docker-from-docker:1": {
            "version": "latest"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.Go"
            ]
        }
    },
    "forwardPorts": [8080],
    "remoteUser": "vscode"
}