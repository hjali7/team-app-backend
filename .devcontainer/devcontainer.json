{
    "name": "Go Backend (w/DB)",
    "dockerComposeFile": "docker-compose.yml",
    "service": "app" ,
    "workspaceFolder": "/workspaces/team-app-backend",
    "features": {
        "ghcr.io/devcontainers/features/docker-from-docker:1": {
            "version": "latest",
            "moby" : false
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.Go"
            ]
        }
    },
    "remoteUser": "vscode"
}